      "parameters": {
        "jsCode": "// Extract labels and assignees\nconst items = $input.all();\nconst results = [];\n\nfor (const item of items) {\n  const labels = item.json.labels?.map(l => l.name) || [];\n  const assignees = item.json.assignees?.map(a => a.login) || [];\n  \n  results.push({\n    json: {\n      ...item.json,\n      labelsList: labels.join(', '),\n      assigneesList: assignees.join(', '),\n      isAssigned: assignees.length > 0\n    }\n  });\n}\n\nreturn results;"
      },
