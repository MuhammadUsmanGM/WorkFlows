      "parameters": {
        "jsCode": "// Parse and extract code changes\nconst items = $input.all();\nconst results = [];\n\nfor (const item of items) {\n  const diff = item.json.body || '';\n  const lines = diff.split('\\n');\n  \n  const codeChanges = {\n    additions: lines.filter(l => l.startsWith('+')).length,\n    deletions: lines.filter(l => l.startsWith('-')).length,\n    files: (diff.match(/diff --git/g) || []).length,\n    diffContent: diff.substring(0, 2000) // First 2000 chars\n  };\n  \n  results.push({ json: { ...item.json, codeChanges } });\n}\n\nreturn results;"
      },
